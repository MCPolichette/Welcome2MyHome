<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">{{house.place_name}}</h5>
          <p>
            <strong>Created</strong>:{{house.createdAt}}</p>
          <p>
            <img src={{house.place_photo}}></p>
          <p>
            <strong>House ID</strong>: {{house.id}}</p>
          <p>
            <strong>Host Name</strong>: {{house.host_name}}</p>
          <p>
            <strong>House Name</strong>: {{house.place_name}}</p>
          <p>
            <strong>Host Address</strong>: {{house.host_address}} {{house.place_city}}, {{house.place_state}},{{house.place_zip}}</p>
          <p>
            <strong>Host Phone</strong>: {{house.host_phone}}</p>
          <p>
